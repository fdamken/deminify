{
    "manifest_version": 2,
    "name": "Deminify",
    "version": "1.0.0",
    "description": "Google Chrome plugin to remove the '.min' from the file names loading the unminified version of the file (e.g. JavaScript and CSS files).",
    "background": {
        "scripts": ["src/background.js"]
    },
    "browser_action": {
        "default_icon": "src/icongray/icongray128.png"
    },
    "icons": {
        "16": "src/icon/icon16.png",
        "48": "src/icon/icon48.png",
        "128": "src/icon/icon128.png",
        "512": "src/icon/icon512.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
